<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <!-- <link rel='stylesheet' href='/stylesheets/style.css' /> -->
    <script>   console.log("HI") 

function GetSortOrder(prop) {    
  return function(a, b) {    
      if (a[prop] > b[prop]) {    
          return 1;    
      } else if (a[prop] < b[prop]) {    
          return -1;    
      }    
      return 0;    
  }    
}    
  
array.sort(GetSortOrder("employeesArray.First_Name")); //Pass the attribute to be sorted on    
document.write("Sorted Employee Names : ");    
for (var item in array) {    
  document.write("<br>" + array[item].First_Name);    
}    

    
    
    
    
    </script>
  </head>
  <body>
    <% employeesArray.forEach(function(eachemployee){

     %>
    <h1><%= JSON.stringify(eachemployee)
      
      
      
      
      %></h1>

    <p>Welcome to <%= title %></p>
  <%}) %>
  </body>
</html>
